<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Button?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<VBox fx:controller="com.property.manager.Views.ShowDetailsOfBranchController" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <Button contentDisplay="TOP" mnemonicParsing="false" prefHeight="20" prefWidth="20" styleClass="back-button">
            <graphic>
               <FontAwesomeIconView glyphName="CHEVRON_CIRCLE_LEFT" size="20" styleClass="back-button-icon" />
            </graphic>
         </Button>
         <Text fill="White" strokeType="OUTSIDE" strokeWidth="0.0" text=" Add Branch" textAlignment="CENTER">
            <font>
               <Font name="Bodoni MT" size="46" />
            </font>
         </Text>
      </HBox>
      <HBox>
         <children>
            <TableView fx:id="table" HBox.hgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="name" text="Name" />
                  <TableColumn fx:id="address" text="Address" />
                  <TableColumn fx:id="phoneNumber" text="Phone Number" />
                  <TableColumn fx:id="email" text="Email" />
                  <TableColumn fx:id="webAddress" text="Web address" />
                  <TableColumn fx:id="userName" text="Username" />
                  <TableColumn fx:id="password" text="Password" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </children>
</VBox>
